package kosa.basic;

public class LoopExam02 {

	public static void main(String[] args) {
		// 중첩 for문
		// 전체 구구단 출력 (사용하지않으면 계속 작성해야함)
		// 중첩 for문을 이용해서 구구단 출력
//		for (int i = 2; i <= 9; i++) {
//			for (int j = 1; j <= 9; j++) {
//				System.out.println(i+"*"+j+"="+(i*j));
//			}
//		}
		
		//퀴즈> 4x + 5y = 60 => x,y를 구하자(모든 경우의 수)
		for (int x = 0; x <=15; x++) {
			for (int y = 0; y <=12; y++) {
				if((4*x)+(5*y)==60) {
					System.out.println(x+"와"+y);
				}
			}
		}
		
//		if((4*x)+(5*y)==60) {
//			for (i < args.length; i++) {
//				
